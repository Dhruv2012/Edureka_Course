import java.util.Scanner;  // Import the Scanner class
abstract class account{
	long accountno;
	double amount;
	String name;
	abstract void withdraw(double amt);
	abstract void deposit(double amt);
}

public class sb_account extends account{
	float interest;
	public void withdraw(float amt){
		
	}
	public void deposit(float amt){
		
	}
}

public class current_account extends sb_account{
	public void withdraw(float amt){
		
	}
	public void deposit(float amt){
		
	}
}
public class casestudy1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner myObj = new Scanner(System.in);  // Create a Scanner object
	    System.out.println("       Enter account type:");
	    String account_type = myObj.nextLine();  // Read user input
	    System.out.println("account type is: " + account_type);  // Output user input
	    
	}

}
